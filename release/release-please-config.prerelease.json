{
	"$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
	"description": "Auto-Generated Release Please configuration for prerelease",
	"pull-request-title-pattern": "chore: prerelease release prepared",
	"pull-request-header": "chore: prerelease release prepared",
	"pull-request-footer": "This PR was generated by the prepare-release GHA",
	"release-type": "node",
	"initial-version": "0.0.1",
	"versioning": "prerelease",
	"prerelease": true,
	"prerelease-type": "next",
	"packages": {
		"packages/telemetry-models": {
			"package-name": "telemetry-models",
			"changelog-path": "docs/changelog.md",
			"extra-files": [
				"src/index.ts"
			]
		},
		"packages/telemetry-connector-entity-storage": {
			"package-name": "telemetry-connector-entity-storage",
			"changelog-path": "docs/changelog.md",
			"extra-files": [
				"src/index.ts"
			]
		},
		"packages/telemetry-service": {
			"package-name": "telemetry-service",
			"changelog-path": "docs/changelog.md",
			"extra-files": [
				"src/index.ts"
			]
		},
		"packages/telemetry-rest-client": {
			"package-name": "telemetry-rest-client",
			"changelog-path": "docs/changelog.md",
			"extra-files": [
				"src/index.ts"
			]
		}
	},
	"plugins": [
		"node-workspace",
		{
			"type": "linked-versions",
			"groupName": "repo",
			"components": [
				"telemetry-models",
				"telemetry-connector-entity-storage",
				"telemetry-service",
				"telemetry-rest-client"
			]
		}
	]
}
